{
  "name": "live-doc-logger",
  "displayName": "Live-Doc-Logger",
  "publisher": "oitashg",
  "description": "Log issues and solutions directly into project docs as you code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "live-doc-logger.DocLog",
        "title": "Live Doc Logger"
      },
      {
        "command": "live-doc-logger.DocLogOpen",
        "title": "Navigate to Documentation"
      },
      {
        "command": "live-doc-logger.DocLogEdit",
        "title": "Edit Documentation"
      },
      {
        "command": "live-doc-logger.DocLogDelete",
        "title": "Delete Documentation"
      },
      {
        "command": "live-doc-logger.showCommands",
        "title": "Show Command Palette"
      },
      {
        "command": "live-doc-logger.DocLogGoToSource",
        "title": "Go Back"
      }
    ]
  },
  "menus": {
    "editor": [
      {
        "command": "live-doc-logger.DocLog",
        "when": "editorHasSelection"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}
